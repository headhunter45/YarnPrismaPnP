{
  "name": "tapper",
  "packageManager": "yarn@4.9.2",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "api:build": "yarn workspace @tapper/api build",
    "api:dev": "yarn workspace @tapper/api dev",
    "api:start": "yarn workspace @tapper/api start",
    "clean": "yarn workspaces foreach -Ap run clean",
    "prisma:generate": "yarn workspace @tapper/prisma generate",
    "prisma:reset": "yarn workspace @tapper/prisma reset",
    "prisma:seed": "yarn workspace @tapper/prisma seed",
    "prisma:test": "yarn workspace @tapper/prisma test",
    "prisma:test:build": "yarn workspace @tapper/prisma test:build",
    "prisma:test:generate": "yarn workspace @tapper/prisma test:generate",
    "prisma:test:reset": "yarn workspace @tapper/prisma test:reset",
    "prisma:test:seed": "yarn workspace @tapper/prisma test:seed"
  },
  "devDependencies": {
    "@yarnpkg/pnpify": "^4.1.5",
    "esprima": "^4.0.1",
    "supports-color": "^10.0.0"
  }
}
